page{
  background-color: #f5f5f5;
}

.times{
  width: 100%;
  height: 300rpx;
  margin-top: 30rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}

.times .title {
  width: 90%;
  height: 10%;
}

.times .time-url {
  width: 90%;
  height: 80%;
  border-radius: 10rpx;
}

.times .time-url image{
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}